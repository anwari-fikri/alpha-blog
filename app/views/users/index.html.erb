<div class="container">
  <div class="row">
    <div class="col-2">
      <%= render 'shared/main_nav' %>
    </div>
    <div class="col-7">
      <%= render "shared/return_nav", obj: articles_path, txt: "Check out these users" %>
      <div class="container border pt-4 pb-4">
        <%= render 'user_list' %>
      </div>
      <div class="d-flex justify-content-center bd-highlight mt-3">
        <ul class="pagination">
          <li><%= will_paginate @users, class: "page-link" %></li>
        </ul>
      </div>
    </div>
    <div class="col">
      <%= render 'shared/side_nav' %>
    </div>
  </div>
</div>